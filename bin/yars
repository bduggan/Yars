#!/usr/bin/perl

# PODNAME: yars
# ABSTRACT: Yet Another RESTful-Archive Service
# VERSION

=head1 SEE ALSO

L<Yars>, L<Yars::Client>, L<yarsclient>

=cut

use FindBin;
use lib "$FindBin::Bin/../lib";

use Yars;
use Clustericious::Commands;

use strict;
use warnings;

$ENV{MOJO_APP} ||= 'Yars';

&main;

sub main {
    my $cmd = $ARGV[0];
    my $app = Yars->new();
    $app = Clustericious::Commands->start;
    $app;
}


